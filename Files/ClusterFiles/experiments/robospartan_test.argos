<?xml version="1.0" ?>
<argos-configuration>
 
 <framework>
   <system threads="0"/>
   <experiment length="54000"
   ticks_per_second="10"
   random_seed="1"/>
 </framework>
 
 <controllers>
   <psiswarm_dps id="psiswarm_dps"
     library="build/controllers/psiswarm_dps/libpsiswarm_dps">
     <actuators>
       <psiswarm_wheels implementation="default"/>
     </actuators>
     <sensors>
      <psiswarm_proximity implementation="default" show_rays="true"/>
     </sensors>
     <params>
       <wheel_turning hard_turn_angle_threshold="70"
         soft_turn_angle_threshold="25"
        no_turn_angle_threshold="12.5"
         max_speed="4.0" />
       <partitioning partition_type="4"
         initial_partition_length="67"
         gain="80"
         alpha="50" 
         epsilon="2"
         memory_factor="3"
         error_type="0" />
     </params>
   </psiswarm_dps>
 </controllers>
 
 <loop_functions library="build/loop_functions/psiswarm_dps_loop_functions/libpsiswarm_dps_loop_functions"
   label="psiswarm_dps_loop_functions" >
   <dps individual="/scratch/ebb505/CPS_SA_Hom/1/1/individual.csv"
   	global="/scratch/ebb505/CPS_SA_Hom/1/1/global.csv"
     transfer_type="1"
     home_source_distance="1.3"/>
 </loop_functions>
 
 <arena size="2.3,2.3,2" center="0,0,1">
   <floor id="floor"
     source="loop_functions"
     pixels_per_meter="50" />
   <box id="wall_north" size="1.6,0.01,0.25" movable="false">
     <body position="0,1.1,0" orientation="0,0,0"/>
   </box>
   <box id="wall_south" size="1.6,0.01,0.25" movable="false">
     <body position="0,-1.1,0" orientation="0,0,0"/>
   </box>
   <box id="wall_east" size="0.01,2.2,0.25" movable="false">
     <body position=".80,0,0" orientation="0,0,0"/>
   </box>
   <box id="wall_west" size="0.01,2.2,0.25" movable="false">
     <body position="-.80,0,0" orientation="0,0,0"/>
   </box>
   <distribute>
     <position method="grid" center=".55,0,0"
       distances="1.10,0.15,0"
       layout= "1,,1" />
     <orientation method="constant" values="180,0,0" />
     <entity quantity= "" max_trials="1">
       <psi-swarm id="psiswarm">
         <controller config="psiswarm_dps" />
       </psi-swarm>
     </entity>
   </distribute>
 </arena>
 
 <physics_engines>
   <dynamics2d id="dyn2d"/>
 </physics_engines>
 <media/>
</argos-configuration>
